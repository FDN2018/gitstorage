    <div class="modal fade text-left" id="viewpricebook" tabindex="-1" role="dialog" aria-labelledby="myModalLabel160"
        aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <h5 class="modal-title white" id="myModalLabel160">
                        Quick View
                    </h5>
                    <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                        <i data-feather="x"></i>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Pricebook Information</h4>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">Pricebook Name</label>
                                    <input type="text" class="form-control" id="basicInput"
                                        placeholder="Pricebook Name" style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">Owner Name</label>
                                    <input type="text" class="form-control" id="basicInput" placeholder="Owner Name"
                                        style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">Status</label>
                                    <input type="text" class="form-control" id="basicInput" placeholder="Status"
                                        style="background-color: white;" disabled>
                                </div>
                                <div class="form-group">
                                    <label for="basicInput">Pricebook For</label>
                                    <input type="text" class="form-control" id="basicInput" placeholder="Pricebook For"
                                        style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">Created By</label>
                                    <input type="text" class="form-control" id="basicInput" placeholder="Created By"
                                        style="background-color: white;" disabled>
                                </div>
                                <div class="form-group">
                                    <label for="basicInput">Created On</label>
                                    <input type="text" class="form-control" id="basicInput"
                                        placeholder="22-Dec-2021" style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">From Date</label>
                                    <input type="text" class="form-control" id="basicInput"
                                        placeholder="22-Dec-2021" style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="basicInput">To Date</label>
                                    <input type="text" class="form-control" id="basicInput" placeholder="22-Dec-2022"
                                        style="background-color: white;" disabled>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
