            <div class="col-md-3">
                <p class="fw-bold m-0 p-0">Search in</p>
                <select id="cols" class="form-control" onchange=colVal(this.value)>
                    <option value="username">Username</option>
                    <option value="fullname">Full Name</option>
                    <option value="trand">Transaction Date</option>
                    <option value="mode">Mode</option>
                    <option value="crdr">Cr/Dr</option>
                    <option value="trantype">Transaction Type</option>                
                    <option value="tramt">Total Transaction Amount</option>
                    <option value="opbal">Opening Balance</option>                
                    <option value="clbal">Closing Balance</option>
                    <option value="operator">LCO Name</option>                    
                    <option value="byuser">By User</option>
                </select>
                
            </div>
            <div class="col-md-3">
                <p class="fw-bold m-0 p-0">Condition</p>
                <select id="cond" class="form-control" style="display: block;" onchange=condVal(this.value)>
                    <option value="any">Anywhere</option>
                    <option value="startwith">Starts With</option>
                    <option value="endwith">Ends With</option>
                    <option value="exactly">Exactly</option>
                    <option value="notlike">Not Like</option>
                </select>
                <select id="condDate" class="form-control" style="display: none;" onchange=condDateVal(this.value)>
                    <option value="equal">=</option>
                    <option value="lessequal">&lt;=</option>
                    <option value="gtequal">&gt;=</option>
                    {{!-- <option value="notequal">!=</option> --}}
                    <option value="between">BETWEEN</option>
                    <option value="thisweek">This Week</option>
                </select>
            </div>
            <div class="col-md-3">
                <div class="form-row">
                    <div class="col" id="ip1" style="display: block;">
                        <p class="fw-bold m-0 p-0">Enter Value</p>
                        <input type="text" class="column_filter form-control" id="testinp" style="display: block;" onchange=dataFetch()>
                    </div>
                    <div class="col" id="ip2" style="display: none;">
                        <p class="fw-bold m-0 p-0" id="date1"></p>
                        <input type="date" class="column_filter form-control" id="testdateinp1" style="display: none;">
                    </div>
                    <div class="col" id="ip3" style="display: none;">
                        <p class="fw-bold m-0 p-0" id="date2"></p>
                        <input type="date" class="column_filter form-control" id="testdateinp2" style="display: none;">
                    </div>
                </div>                    
            </div>
            <div class="col-md-2">
